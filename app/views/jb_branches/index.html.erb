 
<div>
  <h4>
    <%= link_to  new_jb_branch_path, :class => [:btn, :"btn-success"] do |l|%>
      <i class="icon-plus-sign icon-white"></i>
      Add New Branch
    <% end %>
  </h4>

    <h4>
    <%= link_to  new_jb_branch_path, :class => [:btn, :"btn-success"] do |l|%>
      <i class="icon-plus-sign icon-white"></i>
      Add New Franchisee
    <% end %>
  </h4>
<table class="table table-striped table-bordered">
  <thead>
    <th>Branch Name</th>          
    <th>Branch Type</th>
    <th>Short Name</th>
    <th>City</th>
    <th>Address</th>
    <th>Contact Numbers</th>
    <th>Working Hours</th> 
        
  </thead>

<% @jb_branches.each do |jb_branch| %>
  <tr>
    <td><%= jb_branch.branchname %></td>               
    <td><%#= jb_branch.branchtype %></td>
    <td><%#= jb_branch.shortname %></td>
    <td><%= jb_branch.city %></td>
    <td><%#= jb_branch.branchaddress %></td> 
    <td><%= jb_branch.contactnumbers%></td>               
    <td><%#= jb_branch.workinghours %></td>
    <td><%#= jb_branch.maplink%></td>
    <td><%= link_to 'Show', jb_branch %> &nbsp; <%= link_to 'Edit', edit_jb_branch_path(jb_branch) %>
        <%= link_to 'Destroy',jb_branch, method: :delete, data: {confirm: 'Are you sure?'} %>  
    </td>
  </tr>
<% end %>
</table>
<br />
</div>
